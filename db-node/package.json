    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1066.0",
    "express": "^4.17.2",
    "typescript-rest": "^3.0.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92"
  }
}
